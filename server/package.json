    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@typegoose/typegoose": "^10.0.0",
    "@types/multer-gridfs-storage": "^4.0.5",
    "app-root-path": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
